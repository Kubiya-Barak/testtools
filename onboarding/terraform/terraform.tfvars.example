# This is an example file. Copy this to terraform.tfvars and modify the values.

# The API key should be set via environment variable:
# export TF_VAR_kubiya_api_key="your-api-key"

# Organization name to onboard
org_name = "example-org"

# Admin email for the organization
admin_email = "admin@example.com"

# Optional: List of regular users to invite
invite_users = [
  "user1@example.com",
  "user2@example.com"
]

# Optional: List of admin users to invite
invite_admins = [
  "admin1@example.com",
  "admin2@example.com"
]

# Optional: Enable sources
enable_k8s_source = true
enable_github_source = true
enable_jenkins_source = true
enable_jira_source = true
enable_slack_source = true