# This is an example file. Copy this to terraform.tfvars and modify the values.

# The API key should be set via environment variable:
# export TF_VAR_kubiya_api_key="your-api-key"

# Organization details
org_name = "your-org-name"
admin_email = "admin@your-organization.com"

# Optional: Invite additional users
invite_users = [
  "user1@example.com",
  "user2@example.com"
]

# Optional: Invite additional admins
invite_admins = [
  "admin1@example.com",
  "admin2@example.com"
]

# Runner configuration
managed_runner = false  # Set to true to use managed runners